<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, :collection => ['chinese', 'italian', 'japanese', 'french', 'belgian'].sort, :include_blank => false %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
